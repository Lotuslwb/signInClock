<%- include("header.ejs",{"title":'英孚亲子共读训练营'}) %>
<style>
    .medie-player {
        width: 583px;
        margin: auto;
        padding: 0;
        border: 0;
        /* V2 */
        background: rgba(255, 255, 255, 0);
        margin-top: -1px;
    }

    /*定义滚动条高宽及背景 高宽分别对应横竖滚动条的尺寸*/
    .reading-main::-webkit-scrollbar {
        width: 16px;
        height: 16px;
        background-color: #F5F5F5;
    }

    /*定义滚动条轨道 内阴影+圆角*/
    .reading-main::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
        border-radius: 10px;
        background-color: #F5F5F5;
    }

    /*定义滑块 内阴影+圆角*/
    .reading-main::-webkit-scrollbar-thumb {
        border-radius: 10px;
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, .3);
        background-color: #555;
    }

    .record-btn-list {
        display: none;
    }

    .record-btn-list.active {
        display: block;
    }

    .reading-page .record-btn-list .btn-item.btn-item-read {
        display: block;
    }

    .reading-page .record-btn-list.active .btn-item.btn-item-read.record-btn {
        display: block;
    }

    .reading-page {
        overflow: auto;
    }
</style>
<div class="page main-page reading-page index-page active">
    <div class="date-box">
        <div class="date-box-icon"></div>
        <div class="date-box-text"></div>
    </div>

    <div class="reading-title"><%= bookInfo.articleTitle %></div>

    <% if(bookInfo.videoURL){ %>
    <% var videoStrArry = bookInfo.videoURL.split('/'); var videoCode = videoStrArry[videoStrArry.length - 1].split('.html')[0]; %>
    <div class="reading-video">
        <iframe frameborder="0" class="iframe"
                src="https://v.qq.com/iframe/player.html?vid=<%= videoCode %>&tiny=0&auto=0">
        </iframe>
    </div>
    <% }else if(bookInfo.audioURL){ %>

    <div class="medie-player J-medie-player" data-url="<%= bookInfo.audioURL %>">
        <img src="/daka/img/media-player-icon.jpg" class="media-player-icon">
        <div class="articleTitle">
            <%= bookInfo.articleTitle %>
            <span>来自EF英孚教育青少儿英语</span>
        </div>
    </div>
    <% } %>

    <div class="reading-main <%= bookInfo.videoURL ? 'video' : ''; %>">
        <%- bookInfo.articleText %>
    </div>

    <div class="J-loadedbox" style="display: none;">

        <!-- 无打卡 无录音 -->
        <div class="record-btn-list record-btn-list-new" id="case-1">
            <div class="record-btn btn-item btn-item-read">
                <div class="J-record-btn game_time">
                    <img src="/daka/img/V2/recodeicon.png" class="record-icon">
                </div>
                <div class="record-status J-record-status J-record-status2">点击录制</div>
                <div>可以选择你喜欢的一段进行录音</div>
            </div>
        </div>

        <!-- 有录音 && 可以继续打卡 -->
        <div class="record-btn-list record-btn-list-new" id="case-2">
            <div class="playRecord-btn btn-item btn-item2 btn-item-read">
                <div class="J-playRecord-btn game_time">
                    <img src="/daka/img/V2/replayicon.png" class="record-icon">
                </div>
                <div class="record-status J-playRecord-text">回放</div>
            </div>
            <div class="againRecord-btn btn-item btn-item2 btn-item-read">
                <img src="/daka/img/V2/recodeicon.png" class="record-icon J-againRecord-btn">
                <div class="record-status J-record-status">重录</div>
            </div>
            <div class="gotoDaka-btn btn-item btn-item2 btn-item-read">
                <img src="../../../../daka/img/V2/gotoDakaIcon.png" class="record-icon J-gotoDaka-btn">
                <div class="record-status gotoDaka-btn-text">去打卡</div>
            </div>
        </div>


        <!-- type && type == 'read'  已打卡 -->
        <div class="record-btn-list record-btn-list-new" id="case-3">
            <div class="btn-item btn-item-read">
                <div class="J-playRecord-btn game_time">
                    <img src="/daka/img/V2/replayicon.png" class="record-icon">
                </div>
                <div class="record-status J-playRecord-text">回放</div>
            </div>
            <div class="btn-item btn-item-read">
                <div class="J-record-btn2 game_time">
                    <img src="/daka/img/V2/recodeicon.png" class="record-icon">
                </div>
                <div class="record-status J-record-status">重录</div>
            </div>
            <div class="btn-item btn-item-read">
                <img src="/daka/img/V2/shareicon.png" class="record-icon J-share-btn">
                <div class="record-status">分享</div>
            </div>
        </div>


        <!-- 未打卡 有录音 && 不能再打卡 -->
        <div class="record-btn-list record-btn-list-new" id="case-4">
            <div class="playRecord-btn btn-item btn-item2 btn-item-read">
                <div class="J-playRecord-btn game_time">
                    <img src="/daka/img/V2/replayicon.png" class="record-icon">
                </div>
                <div class="record-status J-playRecord-text">回放</div>
            </div>
            <div class="againRecord-btn btn-item btn-item2 btn-item-read">
                <img src="/daka/img/V2/recodeicon.png" class="record-icon J-againRecord-btn">
                <div class="record-status J-record-status">重录</div>
            </div>
            <div class="gotoShare-btn btn-item btn-item-read">
                <img src="/daka/img/V2/shareicon.png" class="record-icon J-share-btn">
                <div class="record-status">分享</div>
            </div>
        </div>

    </div>

    <!--<div class="banner">-->
    <!--<img src="../../../../daka/img/pic-banner.jpg">-->
    <!--</div>-->
</div>

<div class="overlay overlay-share">
    <img src="/daka/img/shareTxt.png" class="shareTxt">
