    function recordStop(res) {
        localId = res.localId;
//        $('.record-btn-list').addClass('active');
        countStop();
    }

    var timeInLocal = localStorage.getItem('time');

    function getFormatDate(date) {
        var now = new Date(date);
        var year = now.getFullYear();
        var month = now.getMonth() + 1;
        var day = now.getDate();

        month = month <= 9 ? ('0' + month) : ('' + month);
        day = day <= 9 ? ('0' + day) : ('' + day);

        return year + month + day;
    }


    initWX(function () {

        if (!hasVoice && !hasRead) {
            initWXSahre();
        } else {
            initReadWXShare();
        }


        if (recordLocalId) {
            $('.J-loadedbox').show();
            //音频在本地
            var recordLocalURL = "/files/media/" + recordServerId + ".mp3";
            var audioElement2;
            musicPlayer();
            function musicPlayer() {
                var musicSrc = recordLocalURL;
                audioElement2 = new Audio(musicSrc);
                audioElement2.controls = false;
                audioElement2.loop = false;
                audioElement2.autoplay = false;
                audioElement2.load();
                audioElement2.pause();

                //bind event
                $('.J-playRecord-btn').click(function () {

                    if ($(this).hasClass('active')) {
                        $(this).removeClass('active');
                        audioElement2.pause();
                        $('.J-playRecord-text').text('回放');
                    } else {
                        $(this).addClass('active');
                        audioElement2.play();
                        $('.J-playRecord-text').html('回放中<span class="dotting"></span>');
                    }
                }).removeClass('active');

            }
        } else {
            //音频还在微信服务器上
            if (recordServerId) {
                WXDownloadVoice(function () {
                    $('.J-loadedbox').show();
                }, function () {
                    $('.J-loadedbox').show();
                });
            } else {
                $('.J-loadedbox').show();
            }

            //播放录音  回放
            $('.J-playRecord-btn').click(function () {
                audioElement.pause();

                if ($(this).hasClass('active')) {
                    $(this).removeClass('active');
                    wx.stopVoice({
                        localId: localId
                    });
                    $('.J-playRecord-text').text('回放');
                } else {
                    $(this).addClass('active');
                    if (localId) {
                        wx.playVoice({
                            localId: localId // 需要播放的音频的本地ID，由stopRecord接口获得
                        });
