                                .index() == 0 ? "M" : "F";
                            customer.Comments = '用户标签属性：' + me.tagDes + ';' + levelMap[me
                                .level] + '|就读中心：' + $('.J-center').val();
                            customer.City = moblieInfo.city;
                            customer.Email = email ? email : customer.MobilePhone +
                                "@noemail.com";


